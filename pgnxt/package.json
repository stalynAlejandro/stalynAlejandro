  "scripts": {
    "start": "react-scripts start",
    "start-https": "set HTTPS=true&&npm start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "npm test -- --coverage --watchAll=false --reporters=default --reporters=jest-sonar",
    "test-once": "npm test -- --no-cache --watchAll=false",
    "test-ci": "node --expose-gc node_modules/react-scripts/scripts/test.js --no-cache --watchAll=false --env=jsdom --runInBand --logHeapUsage --reporters=default --reporters=jest-sonar --coverage ",
    "eject": "react-scripts eject",
    "lint": "npx eslint --fix --ext .jsx --ext .js --ext .tsx --ext .ts src",
    "lint:fix": "npm run lint -- --fix"
  },
  "proxy": "http://localhost:8094",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "/types/",
      "<rootDir>/src/i18n.js",
      "<rootDir>/src/index.tsx",
      "<rootDir>/src/reportWebVitals.ts",
      "<rootDir>/src/authConfig.ts",
      "<rootDir>/src/constants/",
      "<rootDir>/src/components/TestComponent",
      "<rootDir>/src/locales/"
    ],
    "coverageReporters": [
      "lcov",
      "json",
      "html"
    ],
    "globalSetup": "<rootDir>/src/global-setup.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@types/styled-components": "5.1.25",
    "concurrently": "7.1.0",
    "eslint": "8.34.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "jest-sonar": "0.2.15",
    "msal-react-tester": "0.1.2",
    "weak-napi": "2.0.2"
  }
